{
  "all": true,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "test/**",
    "node_modules/**",
    "**/*.test.js",
    "**/*.spec.js"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "check-coverage": true,
  "lines": 80,
  "functions": 75,
  "branches": 70,
  "statements": 80,
  "report-dir": "coverage"
}
